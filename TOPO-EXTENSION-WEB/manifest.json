{
  "manifest_version": 3,
  "name": "TOPO",
  "version": "1.6.4.0",
  "_comment": "Version (MAJOR), Ajout de fonctionnalité/Correction BUG Majeur (MINOR), Correction BUG Mineur/Modif fonctionnalité(PATCH), Esthétique",
  "description": "Extension qui permet d'extraire des éléments de l'outil WASAC",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "scripting",
    "downloads",
    "activeTab",
    "webNavigation",
    "cookies",
    "offscreen"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["images/icon128.png"],
      "matches": ["<all_urls>"]
    }
  ]
}